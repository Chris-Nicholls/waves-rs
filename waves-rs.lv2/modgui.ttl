@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/Chris-Nicholls/waves-lv2-rust>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-waves.html> ;
        modgui:stylesheet <modgui/stylesheet-waves.css> ;
        modgui:screenshot <modgui/screenshot-waves.png> ;
        modgui:thumbnail <modgui/thumbnail-waves.png> ;
        modgui:brand "Rustic" ;
        modgui:label "Waves" ;
        modgui:model "lata" ;
        modgui:panel "7-knobs" ;
        modgui:color "blue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "attack" ;
            lv2:name "attack" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "decay" ;
            lv2:name "decay" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "sustain" ;
            lv2:name "sustain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "release" ;
            lv2:name "release" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "level" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "threshold" ;
            lv2:name "threshold" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "waveform" ;
            lv2:name "waveform" ;
        ] ;
    ] .