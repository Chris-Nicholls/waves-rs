@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .


<https://github.com/Chris-Nicholls/waves-lv2-rust>
        a lv2:Plugin, lv2:SpectralPlugin;

        doap:name "Waves" ;
        lv2:minorVersion 0;
        lv2:microVersion 2;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:AudioPort ,
                        lv2:InputPort ;
                lv2:index 0 ;
                lv2:symbol "in" ;
                lv2:name "In"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 1 ;
                lv2:symbol "out" ;
                lv2:name "Out"
        ],[
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "level" ;
                lv2:name "Level";
                lv2:default 0.0 ;
                lv2:minimum -40;
                lv2:maximum 40 ;
                units:unit units:db ;
                lv2:scalePoint [
                        rdfs:label "0" ;
                        rdf:value 0.0
                ] , [
                        rdfs:label "-5" ;
                        rdf:value -5.0
                ] , [
                        rdfs:label "-10" ;
                        rdf:value -10.0
                ]
        ],[
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "threshold" ;
                lv2:name "threshold";
                lv2:default 0 ;
                lv2:minimum -1 ;
                lv2:maximum 1 ;
        ],[
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "attack" ;
                lv2:name "attack";
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 5 ;
        ],
        [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "decay" ;
                lv2:name "decay";
                lv2:default 0.7 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
        ],
        [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "sustain" ;
                lv2:name "sustain";
                lv2:default 0.4 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
        ],
        [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "release" ;
                lv2:name "release";
                lv2:default 0.5 ;
                lv2:minimum 0 ;
                lv2:maximum 5 ;
        ],[
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "undersample" ;
                lv2:name "undersample";
                lv2:portProperty lv2:integer;
                lv2:portProperty lv2:enumeration;
                lv2:default 2 ;
                lv2:minimum 0 ;
                lv2:maximum 4 ;

                lv2:scalePoint [
                        rdfs:label "None" ;
                        rdf:value 0
                ] , [
                        rdfs:label "2x" ;
                        rdf:value 1
                ] , [
                        rdfs:label "4x" ;
                        rdf:value 2
                ] , [
                        rdfs:label "8x" ;
                        rdf:value 3
                ]
        ],[
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "waveform" ;
                lv2:name "waveform";
                lv2:portProperty lv2:integer;
                lv2:portProperty lv2:enumeration;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 4 ;

                lv2:scalePoint [
                        rdfs:label "Sin" ;
                        rdf:value 0
                ] , [
                        rdfs:label "Square" ;
                        rdf:value 1
                ] , [
                        rdfs:label "Triangle" ;
                        rdf:value 2
                ] , [
                        rdfs:label "Saw" ;
                        rdf:value 3
                ]
        ]
        .
